{
  "name": "typescript-react-dev-template",
  "version": "1.0.0",
  "description": "Boilerplate for TypeScript-React project. No backend or deployment configuration.",
  "main": "./dist/bundle.js",
  "scripts": {
    "start": "npm run build && http-server ./dist",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=production webpack --config ./webpack.production.ts",
    "dev": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=development webpack-dev-server --config ./webpack.development.ts",
    "compile": "tsc -p ./",
    "compile-watch": "tsc --watch -p ./",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MajorLift/typescript-react-dev-template.git"
  },
  "author": "MajorLift",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MajorLift/typescript-react-dev-template/issues"
  },
  "homepage": "https://github.com/MajorLift/typescript-react-dev-template#readme",
  "devDependencies": {
